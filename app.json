{
  "expo": {
    "name": "LuxRead",
    "slug": "luxread",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/icon.png",
      "backgroundColor": "#ffffff",
      "resizeMode": "contain"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.alekssvelvelis.LuxRead"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "notification": {
        "icon": "./assets/images/notification-icon.png",
        "color": "#000000"
      },
      "package": "com.alekssvelvelis.LuxRead"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-notifications",
      [
        "expo-splash-screen",
        {
          "android": {
            "preventAutoHide": true,
            "image": "./assets/images/icon.png",
            "backgroundColor": "#ffffff",
            "resizeMode": "contain"
          },
          "ios": {
            "preventAutoHide": true,
            "image": "./assets/images/icon.png",
            "backgroundColor": "#ffffff",
            "resizeMode": "cover"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "a9471553-7c4d-4062-a1bd-89cb515bc12c"
      }
    },
    "owner": "alekssvelvelis"
  }
}
